	#region 2_15
	Console.Write("Введите количество элементов в массиве A: ");
        n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        Console.WriteLine("Введите элементы массива A: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
        }
        Console.Write("Введите количество элементов в массиве B: ");
        int m = Int32.Parse(Console.ReadLine());
        b = new double[m];
        Console.WriteLine("Введите элементы массива B: ");
        for (int i = 0; i < m; i++)
        {
            b[i] = double.Parse(Console.ReadLine());
        }
        Console.Write("Введите число k: ");
        int kkk = Int32.Parse(Console.ReadLine());
        double[] ab = new double[n + m];
        for (int i = 0; i < kkk; i++)
        {
            ab[i] = a[i];
        }
        for (int i = kkk; i < m + kkk; i++)
        {
            ab[i] = b[i - kkk];
        }
        for (int i = m + kkk; i < n + m; i++)
        {
            ab[i] = a[i - m];
        }
        Console.Write("Результат: ");
        for (int i = 0; i < n + m; i++)
        {
            Console.Write(ab[i] + " ");
        }
    	#endregion

	#region 2_16
	Console.Write("Введите количество элементов в массиве: ");
        n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        sum = 0;
        c = 0;
        t = 0;
        Console.WriteLine("Введите элементы массива: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
            sum += a[i];
        }
        c = sum / n;
        for (int i = 0; i < n; i++)
        {
            if (a[i] < c)
            {
                t++;
            }
        }
        int[] ai = new int[t];
        j = 0;
        Console.WriteLine("Результат: ");
        for (int i = 0; i < n; i++)
        {
            if (a[i] < c)
            {
                ai[j] = i;
                Console.Write(ai[j] + " ");
                j++;
            }
        }
    	#endregion

	#region 2_17
	Console.Write("Введите количество элементов в массиве: ");
        n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        max = -1 * Math.Pow(10, 100);
        min = Math.Pow(10, 100);
        double sum1 = 0, sum2 = 0;
        c = 0;
        i1 = -1;
        i2 = -1;
        int k1 = 0, k2 = 0;
        Console.WriteLine("Введите элементы массива: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
            if (a[i] > max)
            {
                max = a[i];
                i1 = i;
            }
            if (a[i] < min)
            {
                min = a[i];
                i2 = i;
            }
            if (a[i] > 0)
            {
                sum1 += a[i];
                k1++;
            }
            else if (a[i] < 0)
            {
                sum2 += a[i];
                k2++;
            }
        }
        for (int i = i1 + 1; i < n; i++)
        {
            if (a[i] < min)
            {
                min = a[i];
                i2 = i;
            }
        }
        if (i1 > i2)
        {
            c = sum1 / k1;
        }
        else
        {
            c = sum2 / k2;
        }

        Console.WriteLine("Результат: ");
        Console.Write(c);
    	#endregion

	#region 2_18
	Console.Write("Введите количество элементов в массиве: ");
        int n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        double max1 = -1 * Math.Pow(10, 100), max2 = -1 * Math.Pow(10, 100);
        int start, end;
        Console.WriteLine("Введите элементы массива: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
            if (i % 2 == 0 & a[i] > max1) max1 = a[i];
            if (i % 2 == 1 & a[i] > max2) max2 = a[i];
        }
        if (max1 > max2)
        {
            start = 0;
            end = (n - n % 2) / 2;
        }
        else
        {
            start = (n - n % 2) / 2;
            end = n;
        }

        for (int i = start; i < end; i++)
        {
            a[i] = 0;
        }

        Console.WriteLine("Результат: ");
        for (int i = 0; i < n; i++)
        {
            Console.Write(a[i] + " ");
        }
    	#endregion

	#region 2_19
	Console.Write("Введите количество элементов в массиве: ");
        n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        max = -1 * Math.Pow(10, 100);
        sum = 0;
        index = -1;
        Console.WriteLine("Введите элементы массива: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
            sum += a[i];
            if (a[i] > max)
            {
                max = a[i];
                index = i;
            }
        }
        if (max > sum) a[index] = 0;
        else a[index] *= 2;
        Console.WriteLine("Результат: ");
        for (int i = 0; i < n; i++)
        {
            Console.Write(a[i] + " ");
        }
    	#endregion

	#region 2_20
	Console.Write("Введите количество элементов в массиве: ");
        n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        min = Math.Pow(10, 100);
        sum = 0;
        i1 = -1;
        i2 = -1;
        start = -1;
        flag = true;
        Console.WriteLine("Введите элементы массива: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
            if (a[i] < min)
            {
                min = a[i];
                i1 = i;
            }
            if (a[i] < 0 & flag == true)
            {
                i2 = i;
                flag = false;
            }
        }
        if (i2 < i1) start = 0;
        else start = 1;
        for (int i = start; i < n; i += 2)
        {
            sum += a[i];
        }
        Console.Write("Результат: ");
        Console.Write(sum);
    	#endregion

	#region 3_2
	Console.Write("Введите количество элементов в массиве: ");
        n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        max = -1 * Math.Pow(10, 100);
        sum = 0;
        int km = 1;
        Console.WriteLine("Введите элементы массива: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
            if (a[i] > max)
            max = a[i];
        }
        for (int i = 0; i < n; i++)
        {
            if (a[i] == max)
            {
                a[i] += km;
                km++;
            }
        }

        Console.Write("Результат: ");
        for (int i = 0; i < n; i++)
        {
            Console.Write(a[i] + " ");
        }
	#endregion
	
	#region 3_4
	Console.Write("Введите количество элементов в массиве: ");
        n = Int32.Parse(Console.ReadLine());
        a = new double[n];
        max = -1 * Math.Pow(10, 100);
        sum = 0;
        Console.WriteLine("Введите элементы массива: ");
        for (int i = 0; i < n; i++)
        {
            a[i] = double.Parse(Console.ReadLine());
            if (a[i] > max)
            max = a[i];
        }
        for (int i = 0; i < n; i++)
        {
            sum += a[i];
            if (a[i] == max)
            {
                a[i] = sum - a[i];
            }
        }

        Console.Write("Результат: ");
        for (int i = 0; i < n; i++)
        {
            Console.Write(a[i] + " ");
        }
	#endregion

	
        Console.ReadKey();
    }
}
